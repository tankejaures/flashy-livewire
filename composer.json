{
    "name": "tyjurkes/flashy",
    "description": "Easy flash notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Jaures Tankeu",
            "email": "tankeujaures25@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0|~6.0|~7.0|~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-0": {
            "Tyjurkes\\Flashy": "src/"
        },
        "files": [
            "src/Tyjurkes/Flashy/functions.php"
        ]
    },
    "minimum-stability": "stable",
    "extra" : {
        "laravel" : {
            "providers" : [
                "Tyjurkes\\Flashy\\FlashyServiceProvider"
            ],
            "aliases": {
                "Flashy": "Tyjurkes\\Flashy\\Flashy"
            }
        }
    }
}
